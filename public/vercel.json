{
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "dest": "/api/index.py" },
    { "source": "/dashboard", "dest": "/public/dashboard/index.html" },
    { "source": "/dashboard/(.*)", "dest": "/public/dashboard/index.html" },
    { "source": "/", "dest": "/public/index.html" }
  ]
}
